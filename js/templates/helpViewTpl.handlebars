                          &quot;id&quot; : &quot;1&quot;,
                          &quot;st_extent&quot; : &quot;BOX(-24.00125 65.9995833333333,72.00125 82.0004166666667)&quot;,
                          &quot;bbox_north&quot; : &quot;82.0004166666667&quot;,
                          &quot;bbox_east&quot; : &quot;72.00125&quot;
                        }
